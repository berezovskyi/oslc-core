@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rddl: <http://www.rddl.org/>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix dcterms: <http://purl.org/dc/terms/>.

<http://open-services.net/ns/core#> a owl:Ontology;
        dcterms:description "OSLC Dialog contributions."^^rdf:XMLLiteral;
	rdfs:comment "The OSLC Dialog Vocabulary";
	rdfs:label "OSLC Core";
	rdfs:seeAlso
                <http://docs.oasis-open.org/oslc-core/>,
                <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/dialogs-v3.html>,
		<http://open-services.net/ns/core/dialogs-vocabulary.ttl>.

<http://open-services.net/ns/core#Dialog> a rdfs:Class;
	rdfs:comment "Describes information about a dialog such as its title and dimensions";
	rdfs:isDefinedBy <http://open-services.net/ns/core#>;
	rdfs:label "Dialog".

<http://open-services.net/ns/core#creationDialog> a rdf:Property;
	rdfs:comment "Enables clients to create a resource via a UI.";
	rdfs:isDefinedBy <http://open-services.net/ns/core#>;
	rdfs:label "creationDialog".

<http://open-services.net/ns/core#dialog> a rdf:Property;
	rdfs:comment "The URI of the HTML dialog.";
	rdfs:isDefinedBy <http://open-services.net/ns/core#>;
	rdfs:label "dialog".

<http://open-services.net/ns/core#selectionDialog> a rdf:Property;
	rdfs:comment "Enables clients to select one or more resources via a UI.";
	rdfs:isDefinedBy <http://open-services.net/ns/core#>;
	rdfs:label "selectionDialog".

